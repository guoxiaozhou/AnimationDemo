<resources>
    <string name="app_name">CustomProgressView</string>

    <declare-styleable name="circleProgressBar">
        <attr name="circleWidth" format="dimension" />
        <attr name="betaAngle" format="integer" />
        <attr name="firstColor" format="color" />
        <attr name="secondColor" format="color" />
    </declare-styleable>
    <declare-styleable name="progressCircleView">
    <attr name="circleNumberWidth" format="dimension"/>
    <attr name="circleDotWidth" format="dimension"/>
    <attr name="circleRectWidth" format="dimension"/>
    <attr name="circleNumberColor" format="color"/>
    <attr name="circleDotColor" format="color"/>
    <attr name="circleRectColor" format="color"/>
    <attr name="circleColor" format="color"/>
    <attr name="textColor" format="color"/>
    </declare-styleable>

    <color name="defaultcolor">#5CACEE</color>
</resources>
